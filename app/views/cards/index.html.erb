<p id="notice"><%= notice %></p>

<h1>Cards</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cards.each do |card| %>
      <tr>
        
        <td><%= card.name %><%= link_to 'Show', card %></td>
        <td><%= image_tag(card.image_url) %></td>
      
      
      </tr>
    <% end %>
  </tbody>
</table>